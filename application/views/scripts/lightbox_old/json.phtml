<?php

$requestURI = $this->requestURI;
$host = OpenContext_OCConfig::get_host_config();
$AtomRequestURI = $host.(str_replace(".json", ".atom", $requestURI));

$atom_string = file_get_contents($AtomRequestURI);

$Image_object = OpenContext_LightboxOutput::atom_to_array($atom_string);

echo Zend_Json::encode($Image_object);

?>
